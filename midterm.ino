int level = 0;
int btn = 0;
int val = 0;
int height = 0;
int flyheight = 0;
int moheight = 20;
int kill = 0;
boolean nextch = true;
boolean nextat = false;
boolean nextmo = false;
boolean nextbc = true;
boolean die = false;
boolean entermo = false;
boolean repeat = true;
void loop() {

  while (level == 0) {

    title();
    if (digitalRead(btn1)) {
      level = 1;
    }
  }
  delay(300);
  display.clear();
  while (level == 1) {
    if (flyheight >= 0) {
      flyheight = 0;
      display.clear();
      display.drawXbm(0, flyheight, 128, 64, fly1);
      display.setFont(ArialMT_Plain_10);
      display.drawString(0, 0, "press");
      display.drawString(0, 10, "button!!");
      display.display();
      delay(200);
      if (digitalRead(btn1)) {
        flyheight -= 10;
      }
    }
    else if (flyheight > -64) {
      display.clear();
      display.drawXbm(0, flyheight, 128, 64, fly1);
      display.display();
      delay(2000 / (-flyheight));
      display.clear();
      display.drawXbm(0, flyheight + 1, 128, 64, fly2);
      display.display();
      delay(2000 / (-flyheight));
      if (digitalRead(btn1)) {
        flyheight -= 10;
      }
      flyheight += 5;
    }
    else {
      level = 2;
    }
  }
  while (level == 2) {
    val = analogRead(height1);
    height = map(val, 0, 4096, 0, 44);

    int atPos = 88;
    int moPos = -20;
    for (int i = 0; i < 2; i++) {
      if (nextbc) {
        display.drawXbm(0, 0, 128, 64, background1);
        display.drawXbm(108, height, 20, 20, ch1);
      }
      if (nextat) {
        display.drawXbm(atPos - (i * 15), height + 10, 10, 10, at);
      }
      if (entermo) {
        display.drawXbm(moPos + (i * 5), moheight, 30, 30, mo);
      }
      if (nextmo) {
        display.drawXbm(0, moheight, 30, 30, mo);
      }
      if (die) {
        display.drawXbm(0, moheight, 30, 30, effect1);
      }
      display.display();
      delay(100);
      display.clear();

      if (nextbc) {
        display.drawXbm(0, 0, 128, 64, background2);
        display.drawXbm(108, height, 20, 20, ch2);
      }
      if (nextat) {
        display.drawXbm(atPos - ((i + 1) * 15), height + 10, 10, 10, at);
      }
      if (entermo) {
        display.drawXbm(moPos + ((i + 1) * 5), moheight, 30, 30, mo);
      }
      if (nextmo) {
        display.drawXbm(0, moheight + 5, 30, 30, mo);
      }
      if (die) {
        display.drawXbm(0, moheight, 30, 30, effect2);
      }
      display.display();
      delay(100);
      display.clear();
      atPos -= 30;
      moPos += 10;
    }
    ledcWriteTone(channel, 0);
    if (nextat == 0 && digitalRead(btn1)) {
      nextat = 1;
      ledcWriteTone(channel, 880);
    }
    else {
      nextat = 0;
    }
    if (repeat) {
      die = 0;
    }
    if ((moheight - height) < 10 && (moheight - height) > -10 && nextat && repeat == 0) {
      kill++;
      die = 1;
      nextmo = 0;
      entermo = 0;
      repeat = 1;
    }

    else if ((random(2) == 1) && nextmo == 0 && repeat) {
      entermo = 1;
      nextmo = 0;
      moheight = random(34);
      repeat = 0;
    }
    else if (entermo && nextmo == 0) {
      die = 0;
      entermo = 0;
      nextmo = 1;
    }

    if (kill == 5) {
      
      level = 3;
      display.drawXbm(0, 0, 128, 64, background1);
      display.drawXbm(108, height, 20, 20, ch1);
      display.drawXbm(0, moheight, 30, 30, effect1);
      display.display();
      delay(100);
      display.clear();

      display.drawXbm(0, 0, 128, 64, background2);
      display.drawXbm(108, height, 20, 20, ch2);
      display.drawXbm(0, moheight, 30, 30, effect2);
      display.display();
      delay(100);
      display.clear();

      display.drawXbm(0, 0, 128, 64, background1);
      display.drawXbm(108, height, 20, 20, ch1);
      display.drawXbm(0, moheight, 30, 30, effect2);
      display.display();
      delay(100);
      display.clear();

      display.drawXbm(0, 0, 128, 64, background2);
      display.drawXbm(108, height, 20, 20, ch2);
      display.drawXbm(0, moheight, 30, 30, effect2);
      display.display();
      delay(100);
      display.clear();
      
    }

  }
  while (level == 3) {
    int dis = 108;
    ledcWriteTone(channel, 0);
    for (dis = 108; dis > -68; dis -= 20) {
      display.drawXbm(0, 0, 128, 64, background1);
      display.drawXbm(dis, height, 20, 20, ch1);
      display.display();
      delay(100);
      display.clear();
      display.drawXbm(0, 0, 128, 64, background2);
      display.drawXbm(dis - 10, height, 20, 20, ch2);
      display.display();
      delay(100);
      display.clear();
    }
    level = 4;
  }
  while (level == 4) {
    display.clear();
    display.setFont(ArialMT_Plain_16);
    display.drawString(0, 0, "Congratulation!!");
    display.drawXbm(0, 5, 128, 64, fly1);
    display.display();
    delay(500);
    display.clear();
    display.setFont(ArialMT_Plain_16);
    display.drawString(0, -2, "Congratulation!!");
    display.drawXbm(0, 0, 128, 64, fly2);
    display.display();
    delay(200);
    if (digitalRead(btn1)) {
      level = 2;
      kill = 0;
    }
  }
}
